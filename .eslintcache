Stories = () => {\n    getStory() \n    .then((story) => {\n      setStories(story);\n    });\n  };\n\n  return (\n    <div>\n      <h1>story page</h1>\n      <div>{storyMenuStatus ? <MakeStory /> : \"\"} \n      </div> \n      \n      <div class=\"storyCardGrid\">\n      {stories.map((v, i) => {\n        return (\n          <div class=\"storyCard\">\n            <h2 class=\"storyTitle\"> Title: {v.title}</h2>\n            <h3 class=\"storyWords\">Prompts: {v.wordPrompts.join(\"  \")}</h3> \n            <br></br>\n            <p class=\"storyBody\">{v.body}</p>\n          </div>\n        );\n      })} \n      </div> \n      <button onClick={openCreateMenu}>make Story</button>  \n\n     <div class=\"bookContainer\">\n      <div class=\"loading\"> \n     \n      <div class=\"brownBit1\"> \n      <div class=\"page1\">  \n      ------------------------------\n      </div> \n\n      <div class=\"extraPage1\"> \n        ------------------------------  \n      </div> \n      </div> \n      \n      <div class=\"brownBit2\">  \n      \n      <div class=\"page2\"> \n      ------------------------------\n      </div>  \n\n\n      </div>\n     \n      </div>  \n      </div> \n\n    </div> \n  );\n};\n\nexport default Story;\n","/mnt/c/Users/Jordon/projects/javascript/fully-responsive/src/pages/story/makeStory.js",["74"],"import { useGlobalState } from \"../../config/store\";\nimport { postStory } from \"../../services/storyServices\";\nimport { Draggable, DraggableCore } from \"react-draggable\"; // The default\n\nconst MakeStory = () => {\n  const { store,dispatch } = useGlobalState();\n  const { words } = store;\n\n  const sendStory = (event) => {\n    // event.preventDefault()\n    event.preventDefault();\n    console.log(event.target.title.value);\n\n    const storyObj = {\n      title: event.target.title.value,\n      wordPrompts: words,\n      body: event.target.body.value,\n    };\n\n    postStory(storyObj); \n    \n    dispatch({\n      type: \"setStoryMenuStatus\",\n  